{
	"name": "@suppayami/eslint-config",
	"type": "module",
	"version": "6.0.1",
	"packageManager": "pnpm@10.9.0",
	"description": "Yami's ESLint Config",
	"author": "Dr.Yami <dr.yami.moe@gmail.com> (https://github.com/suppayami)",
	"license": "MIT",
	"homepage": "https://github.com/suppayami/eslint-config",
	"keywords": [
		"eslint-config"
	],
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "tsdown --format esm --clean",
		"stub": "tsdown --format esm",
		"dev": "tsdown --format esm --watch & npx @eslint/config-inspector@latest",
		"lint": "pnpm run stub && eslint .",
		"prepack": "pnpm build",
		"release": "bumpp && pnpm publish",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"eslint": ">=9.5.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "^5.2.1"
	},
	"devDependencies": {
		"@eslint-react/eslint-plugin": "^1.52.5",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.17.2",
		"bumpp": "^10.2.3",
		"eslint": "^9.33.0",
		"eslint-flat-config-utils": "^2.1.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"tsdown": "^0.14.1",
		"typescript": "^5.9.2"
	},
	"pnpm": {
		"overrides": {
			"is-core-module": "npm:@nolyfill/is-core-module@^1"
		}
	}
}
