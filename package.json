{
	"name": "@suppayami/eslint-config",
	"type": "module",
	"version": "4.0.0",
	"packageManager": "pnpm@9.2.0",
	"description": "Yami's ESLint Config",
	"author": "Dr.Yami <dr.yami.moe@gmail.com> (https://github.com/suppayami)",
	"license": "MIT",
	"homepage": "https://github.com/suppayami/eslint-config",
	"keywords": [
		"eslint-config"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "tsup --format esm,cjs --clean --dts",
		"stub": "tsup --format esm",
		"dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
		"lint": "pnpm run stub && eslint .",
		"prepack": "pnpm build",
		"release": "bumpp && pnpm publish",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"eslint": ">=9.5.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "^3.7.0"
	},
	"devDependencies": {
		"@eslint-react/eslint-plugin": "^1.5.30",
		"@types/eslint": "^8.56.11",
		"@types/node": "^20.14.11",
		"bumpp": "^9.4.1",
		"eslint": "^9.7.0",
		"eslint-flat-config-utils": "^0.2.5",
		"eslint-flat-config-viewer": "^0.1.20",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.9",
		"tsup": "^8.2.2",
		"typescript": "^5.5.4"
	},
	"pnpm": {
		"overrides": {
			"is-core-module": "npm:@nolyfill/is-core-module@^1"
		}
	}
}
